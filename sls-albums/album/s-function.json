{
  "name": "album",
  "runtime": "nodejs4.3",
  "description": "Serverless Lambda function for project: sls-albums",
  "customName": false,
  "customRole": false,
  "handler": "album/handler.handler",
  "timeout": 6,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {
    "excludePatterns": []
  },
  "endpoints": [{
    "path": "album",
    "method": "POST",
    "type": "AWS",
    "authorizationType": "none",
    "authorizerFunction": false,
    "apiKeyRequired": false,
    "requestParameters": {},
    "application/json": {
      "name": "$input.params('name')"
    },
    "responses": {
      "400": {
        "statusCode": "400"
      },
      "default": {
        "statusCode": "200",
        "responseParameters": {},
        "responseModels": {
          "application/json;charset=UTF-8": "Empty"
        },
        "responseTemplates": {
          "application/json;charset=UTF-8": ""
        }
      }
    }
  }],
  "events": [],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
